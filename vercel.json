{
    "rewrites": [
        {
            "source": "/api/v1/:path*",
            "destination": "https://your-backend.onrender.com/api/v1/:path*"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                }
            ]
        }
    ]
}